<div class="wrapper">
    <div class="input-content">
<p id="notice"><%= notice %></p>

<h1>Form Fields</h1>
<br>
<table cellspacing="10">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td class="name"><%= test.name %></td>
        <td class="btn"><%= link_to 'Show', test %></td>
        <td class="btn"><%= link_to 'Edit', edit_test_path(test) %></td>
        <td class="btn"><%= link_to 'Destroy', test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<br>
<%= link_to 'Enter New Name', new_test_path, class:"btn"%>
</div>
</div>